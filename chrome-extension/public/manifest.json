{
  "name": "My Apps",
  "description": "Open your go to tabs in one click",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "My Apps",
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon32.png",
      "128": "icon32.png"    
    }
  },
   "host_permissions":[
    "http://*/",
    "https://*/*"
   ],
   "permissions":[
   "scripting",
   "tabs",
   "activeTab"
   ]
}